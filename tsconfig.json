{
	"compilerOptions": {
		"jsx": "react-jsx",
		"moduleResolution": "Bundler",
		"module": "ESNext",
		"target": "ESNext",
		"skipLibCheck": true,
		"strict": true,
		"allowImportingTsExtensions": true,
		"noEmit": true,
		"noUncheckedIndexedAccess": true,
		"incremental": true
	},
	"exclude": ["node_modules", "dist"]
}
